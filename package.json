{
  "name": "linter-pylama",
  "main": "./lib/init",
  "version": "0.10.1",
  "description": "Lint python on the fly, using pylama",
  "repository": "https://github.com/AtomLinter/linter-pylama",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.0.0"
  },
  "package-deps": [
    "linter"
  ],
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  },
  "keywords": [
    "linter",
    "python",
    "pylint",
    "pep8",
    "pep257",
    "pycodestyle",
    "pydocstyle",
    "pyflakes",
    "mccabe",
    "isort",
    "radon"
  ]
}
